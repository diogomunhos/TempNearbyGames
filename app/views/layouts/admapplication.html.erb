<!DOCTYPE html>
<html>
<head>
	<title>Admnin Wahiga</title>
	
	<%= stylesheet_link_tag    'admapplication', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'admapplication', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
	<!--Open Sans Font [ OPTIONAL ]-->
	<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>


	<!--Bootstrap Stylesheet [ REQUIRED ]-->
	<link href="<%= @url_base %>/assets/css/bootstrap.min.css" rel="stylesheet">


	<!--Nifty Stylesheet [ REQUIRED ]-->
	<link href="<%= @url_base %>/assets/css/nifty.min.css" rel="stylesheet">


	<!--Nifty Premium Icon [ DEMONSTRATION ]-->
	<link href="<%= @url_base %>/assets/css/demo/nifty-demo-icons.min.css" rel="stylesheet">


	<!--Demo [ DEMONSTRATION ]-->
	<link href="<%= @url_base %>/assets/css/demo/nifty-demo.min.css" rel="stylesheet">



	<!--Morris.js [ OPTIONAL ]-->
	<link href="<%= @url_base %>/assets/plugins/morris-js/morris.min.css" rel="stylesheet">


	<!--Magic Checkbox [ OPTIONAL ]-->
	<link href="<%= @url_base %>/assets/plugins/magic-check/css/magic-check.min.css" rel="stylesheet">

	<link href="<%= @url_base %>/assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">





	<!--JAVASCRIPT-->
	<!--=================================================-->

	<!--Pace - Page Load Progress Par [OPTIONAL]-->
	<link href="<%= @url_base %>/assets/plugins/pace/pace.min.css" rel="stylesheet">
	<script src="<%= @url_base %>/assets/plugins/pace/pace.min.js"></script>


	<!--jQuery [ REQUIRED ]-->
	<script src="<%= @url_base %>/assets/js/jquery-2.2.4.min.js"></script>


	<!--BootstrapJS [ RECOMMENDED ]-->
	<script src="<%= @url_base %>/assets/js/bootstrap.min.js"></script>


	<!--NiftyJS [ RECOMMENDED ]-->
	<script src="<%= @url_base %>/assets/js/nifty.min.js"></script>






	<!--=================================================-->

	<!--Demo script [ DEMONSTRATION ]-->
	<script src="<%= @url_base %>/assets/js/demo/nifty-demo.min.js"></script>


	<!--Morris.js [ OPTIONAL ]-->
	<script src="<%= @url_base %>/assets/plugins/morris-js/morris.min.js"></script>
	<script src="<%= @url_base %>/assets/plugins/morris-js/raphael-js/raphael.min.js"></script>


	<!--Sparkline [ OPTIONAL ]-->
	<script src="<%= @url_base %>/assets/plugins/sparkline/jquery.sparkline.min.js"></script>


	<!--Specify page [ SAMPLE ]-->
	<script src="<%= @url_base %>/assets/js/demo/dashboard.js"></script>

	<meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<body>
	<div id="container" class="effect aside-float aside-bright mainnav-lg">
		<!--NAVBAR-->
		<!--===================================================-->
		<%= render '/private/template/navbar' %>
		<!--===================================================-->
		<!--END NAVBAR-->

		<div class="boxed">

			<!--CONTENT CONTAINER-->
			<!--===================================================-->
			<%= yield %>
			<!--===================================================-->
			<!--END CONTENT CONTAINER-->



			<!--ASIDE-->
			<!--=================================================== Menu direito -->
			<%= render '/private/template/aside' %> 
			<!--===================================================-->
			<!--END ASIDE-->


			<!--MAIN NAVIGATION  Menu esquerdo -->
			<!--===================================================-->
			<%= render '/private/template/navleft' %>
			<!--===================================================-->
			<!--END MAIN NAVIGATION-->

		</div>



		<!-- FOOTER -->
		<!--===================================================-->
		<%= render 'private/template/footer'%>
		<!--===================================================-->
		<!-- END FOOTER -->


		<!-- SCROLL PAGE BUTTON -->
		<!--===================================================-->
		<button class="scroll-top btn">
			<i class="pci-chevron chevron-up"></i>
		</button>
		<!--===================================================-->



	</div>


</body>

</html>